   "outputs": [],
   "source": [
    "pitch, yaw, cx, cy, cz, r = sympy.symbols('theta_0, theta_1, c_x, c_y, c_z, r')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "R_pitch = sympy.rot_axis1(pitch)\n",
    "R_yaw = sympy.rot_axis2(yaw)\n",
    "\n",
    "R = R_pitch * R_yaw"
   ]
